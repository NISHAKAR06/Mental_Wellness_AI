# AI Service Environment Variables
# Copy this file to .env and update the values

# Server Configuration
PORT=8001
HOST=0.0.0.0
ALLOWED_HOSTS=localhost,127.0.0.1,https://ai-psychologist-service.onrender.com

# API Keys (Replace with actual keys)
GEMINI_API_KEY=your-gemini-api-key-here
GOOGLE_CLOUD_PROJECT=your-gcp-project-id

# Google Cloud Credentials (if using service account)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# OpenAI API (if using as fallback)
OPENAI_API_KEY=your-openai-api-key-here

# Development
DEBUG=true

# CORS Origins
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,https://mentalwellnessai.vercel.app/

# Optional: Redis for caching
REDIS_URL=redis://localhost:6379
