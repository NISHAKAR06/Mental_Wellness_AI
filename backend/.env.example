# Django Environment Variables - Development
# Copy this file to .env and update the values

# Django Security
SECRET_KEY=your-secret-key-here-for-development
DEBUG=True

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/mental_wellness_db

# API Keys (Replace with actual keys)
GEMINI_API_KEY=your-gemini-api-key-here

# Hosts and CORS
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,http://localhost:8080

# Optional: Redis for sessions
REDIS_URL=
